# RISC OS bound messages
$(BUILD_DIR).ob_ss.o.$(COUNTRY)-B01: $(T5).ob_ss.resource.$(COUNTRY).msg
 $(MAKEMESS) -to $(BUILD_DIR).ob_ss.o.$(COUNTRY)-B_tmp -from $? -country $(COUNTRY)
 $(MAKEAOF) -to $@ -from $(BUILD_DIR).ob_ss.o.$(COUNTRY)-B_tmp -symbol rb_ss_msg_weak

OBJS_01_X=$(BUILD_DIR).ob_ss.o.$(COUNTRY)-B01

$(PROD_BUILD_DIR).o.$(COUNTRY).M01: $(OBJS_01_X) $(OBJS_01_S)
 $(LD) $(LDflags_AOF) -o $@ $(OBJS_01_X) $(OBJS_01_S)

