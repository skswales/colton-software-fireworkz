 SMerge $(T5).ob_skel.resource.$(COUNTRY).spr24
 SMerge $(T5).ob_skel.resource.Neutral.spr22
 SMerge $(T5).ob_skel.resource.$(COUNTRY).spr22
 SMerge $(T5).ob_skel.resource.Neutral.tangod.spr_tg22
