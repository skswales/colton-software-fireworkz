# Windows STRINGTABLE
$(T5).$(PROD).Resources.Windows.$(COUNTRY).res00/rci: $(T5).ob_skel.resource.$(COUNTRY).msg
 Basic -quit $(RCGen) -output $@ -from $? -object 00

# RISC OS loaded messages
$(PROD_BUILD_DIR).!$(APPL).Resources.$(COUNTRY).RISC_OS.Messages.Ob00: $(T5).ob_skel.resource.$(COUNTRY).msg
 $(MAKEMESS) -to $@ -from $? -lf -country $(COUNTRY)

# RISC OS loaded sprites
$(PROD_BUILD_DIR).!$(APPL).Resources.$(COUNTRY).RISC_OS.Sprites11.Ob00: \
 $(T5).ob_skel.resource.Neutral.spr24 \
 $(T5).ob_skel.resource.$(COUNTRY).spr24 \
 $(T5).ob_skel.resource.Neutral.spr22 \
 $(T5).ob_skel.resource.$(COUNTRY).spr22 \
 $(T5).ob_skel.resource.Neutral.spr11 \
 $(T5).ob_skel.resource.$(COUNTRY).spr11 \
 $(T5).ob_skel.resource.Neutral.tangod.spr_tg11
 X SNew; ChangeDynamicArea -SpriteSize 128K; SNew
 SMerge $(T5).ob_skel.resource.Neutral.spr24
 SMerge $(T5).ob_skel.resource.$(COUNTRY).spr24
 SMerge $(T5).ob_skel.resource.Neutral.spr22
 SMerge $(T5).ob_skel.resource.$(COUNTRY).spr22
 SMerge $(T5).ob_skel.resource.Neutral.spr11
 SMerge $(T5).ob_skel.resource.$(COUNTRY).spr11
 SMerge $(T5).ob_skel.resource.Neutral.tangod.spr_tg11
 SDelete delete_me
 Remove $@
 SSave $@
 X SNew; ChangeDynamicArea -SpriteSize 8K

$(PROD_BUILD_DIR).!$(APPL).Resources.$(COUNTRY).RISC_OS.Sprites22.Ob00: \
