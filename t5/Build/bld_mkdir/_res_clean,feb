Set Saved$Wipe$Options <Wipe$Options>

Set Wipe$Options "~C F R ~V"

Set t5$Wipe$Dir <Obey$Dir>.^.^.firewrkz.Resources.Windows.%0

X %Wipe <t5$Wipe$Dir>.config/txt

X %Wipe <t5$Wipe$Dir>.res00/rci
X %Wipe <t5$Wipe$Dir>.res01/rci
X %Wipe <t5$Wipe$Dir>.res06/rci
X %Wipe <t5$Wipe$Dir>.resXX/rci

Set Wipe$Options <Saved$Wipe$Options>
