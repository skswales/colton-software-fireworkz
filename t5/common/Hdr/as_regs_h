; Hdr:as_regs_h

        GET     Hdr:ListOpts

        GET     Hdr:CPU.Generic26
        GET     Hdr:CPU.Generic32

;
; APCS-R and APCS-32 register definitions
;

; In:  a1-a4 argument registers
; Out: a1 result, caller assumes a2-a4 trashed

a1      RN      0
a2      RN      1
a3      RN      2
a4      RN      3

; v1-v6 are callee-saved work registers

v1      RN      4
v2      RN      5
v3      RN      6
v4      RN      7
v5      RN      8
v6      RN      9

; these ones are defined in Hdr:CPU.Generic26
;sl      RN      10            ; stack limit
;fp      RN      11            ; frame pointer
;ip      RN      12            ; trashable temp register

sp      RN      13            ; stack pointer (FD)

; FP coprocessor registers
      
F0 FN 0
F1 FN 1
F2 FN 2
F3 FN 3
F4 FN 4
F5 FN 5
F6 FN 6
F7 FN 7

f0 FN 0
f1 FN 1
f2 FN 2
f3 FN 3
f4 FN 4
f5 FN 5
f6 FN 6
f7 FN 7

        END
